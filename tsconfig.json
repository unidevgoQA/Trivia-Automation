    {
        "compilerOptions": {
            "lib":["es2015","ES6", "dom", "es2018.promise"],
            "target": "ESNext",
            "strict": true,
            "module": "commonjs",
            "sourceMap": true,
            "resolveJsonModule": true,
            "types": [
                "node"
                 
            ],
            "baseUrl": ".",
            "paths": {
                "@pages/*": [
                    "pages/loginPage/*",
                    "pages/mainMenuPage/*",
                    "pages/gamesPage/*",
                    "pages/onDemandPage/*",
                    "pages/MobileGamesPage/*"
                ],
                "@fixtures/*": [
                    "fixtures/*"
                ],                
                "@utils/*": [
                    "utils/*"
                ],
                "@testData/*": [
                    "testData/*",
                    "testData/audios/*",
                    "testData/images/*",
                    "testData/videos*"
                    
                ],
                "@base-function/*": [
                    "base-function/*"
                    
                    
                ]
            }
        }
    }

